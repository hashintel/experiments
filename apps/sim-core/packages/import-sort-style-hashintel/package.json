{
  "name": "import-sort-style-hashintel",
  "version": "0.0.1",
  "private": true,
  "description": "HASH's import-sort style",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "prepublishOnly": "tsc -b .",
    "postinstall": "tsc -b .",
    "build": "tsc -b .",
    "build:watch": "tsc -b . -w",
    "fmt": "prettier \"*.{ts,js,json}\" \"{scripts,src}/**/*.{ts,js,json}\" --write; eslint --quiet --fix \"*.{ts,js,json}\" \"{scripts,src}/**/*.{ts,js,json}\"",
    "fmt-check": "prettier \"*.{ts,js,json}\" \"{scripts,src}/**/*.{ts,js,json}\" --check; eslint --quiet \"*.{ts,js,json}\" \"{scripts,src}/**/*.{ts,js,json}\""
  },
  "license": "UNLICENSED",
  "devDependencies": {
    "@types/node": "^14.14.7",
    "import-sort-parser": "^6.0.0",
    "import-sort-style": "^6.0.0",
    "typescript": "^4.1.3"
  }
}
