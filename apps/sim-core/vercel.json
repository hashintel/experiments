{
  "github": {
    "silent": true,
    "autoJobCancelation": true
  },
  "installCommand": "./scripts/install-dependencies.sh",
  "buildCommand": "yarn ws:core build --copy-index-to-root",
  "outputDirectory": "./packages/core/dist",
  "rewrites": [
    {
      "source": "/:match*",
      "destination": "/"
    }
  ]
}
